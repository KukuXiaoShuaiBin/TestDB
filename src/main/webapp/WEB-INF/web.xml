<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>TestDemo</servlet-name>
  	<display-name>TestDemo</display-name>
  	<description></description>
  	<servlet-class>com.my.test.TestDemo</servlet-class>
